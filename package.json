{
  "name": "sentiment-insights-fullstack",
  "version": "1.0.0",
  "description": "Full-stack Sentiment-Driven Product Feature Insights for Manufacturers & Sellers",
  "main": "index.js",
  "scripts": {
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "server": "cd backend && python -m uvicorn main:app --reload --host 0.0.0.0 --port 8000",
    "client": "cd frontend && npm start",
    "build": "cd frontend && npm run build",
    "install-all": "npm install && cd frontend && npm install && cd ../backend && pip install -r requirements.txt",
    "start": "cd backend && python -m uvicorn main:app --host 0.0.0.0 --port 8000",
    "test": "cd frontend && npm test",
    "lint": "cd frontend && npm run lint"
  },
  "keywords": [
    "sentiment-analysis",
    "product-insights",
    "machine-learning",
    "react",
    "fastapi",
    "aws"
  ],
  "author": "Shivam Kumar - IIT Gandhinagar",
  "license": "MIT",
  "devDependencies": {
    "concurrently": "^8.2.2"
  }
}
